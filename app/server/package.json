{
  "name": "node",
  "version": "1.0.0",
  "description": "",
  "main": "app.js",
  "scripts": {
    "start": "npm run build && node dist/app.js",
    "dev": "export ENV=DEV & npx nodemon ./src/app.ts",
    "dev:inspect": "export ENV=DEV & npx nodemon --inspect-brk=0.0.0.0:9229 ./src/app.ts",
    "test": "npm run test:unit && npm run test:integration",
    "test:unit": "ts-mocha --paths --config test/unit/.mocharc.json -p test/tsconfig.json ./test/unit/**/*.[jt]s",
    "test:integration": "ts-mocha --paths --config test/integration/.mocharc.json -p test/tsconfig.json ./test/integration/**/*.[jt]s",
    "seed": "ts-node src/db-seed.js",
    "build": "rimraf ./dist && tsc"
  },
  "keywords": [],
  "author": "",
  "license": "ISC",
  "dependencies": {
    "body-parser": "^1.20.2",
    "cors": "^2.8.5",
    "dotenv": "^16.3.1",
    "express": "^4.18.2",
    "express-async-handler": "^1.2.0",
    "express-mongo-sanitize": "^2.2.0",
    "express-validator": "^7.0.1",
    "http-errors": "^2.0.0",
    "mongodb": "5.6",
    "mongoose": "^7.3.1",
    "tsconfig-paths": "^4.2.0",
    "uuid": "^9.0.0"
  },
  "devDependencies": {
    "@babel/plugin-proposal-private-property-in-object": "^7.21.11",
    "@faker-js/faker": "^8.0.2",
    "@types/chai": "^4.3.5",
    "@types/express": "^4.17.17",
    "@types/jest": "^29.5.3",
    "@types/mocha": "^10.0.1",
    "@types/node": "^20.4.2",
    "@types/sinon": "^10.0.15",
    "@types/uuid": "^9.0.2",
    "chai": "^4.3.7",
    "jest": "^29.6.1",
    "mocha": "^10.2.0",
    "mongodb-memory-server": "^8.13.0",
    "nodemon": "^3.0.1",
    "rimraf": "^5.0.1",
    "sinon": "^15.2.0",
    "ts-jest": "^29.1.1",
    "ts-mocha": "^10.0.0",
    "ts-node": "^10.9.1",
    "typescript": "^5.1.6"
  }
}
